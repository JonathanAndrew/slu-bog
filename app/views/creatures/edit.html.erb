<h3><strong><u>Edit Creature</u></strong></h3>

<%= form_for @creature do |f| %>
	<center><%= f.label :name %></center>
	<center><%= f.text_field :name %></center>

	<center><%= f.label :description %></center>
	<center><%= f.text_area :description %></center>

	<center><%= f.submit %></center>
<% end %>
